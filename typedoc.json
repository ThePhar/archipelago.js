{
  "$schema": "https://typedoc.org/schema.json",
  "name": "archipelago.js",
  "cleanOutputDir": true,
  "excludePrivate": true,
  "excludeInternal": true,
  "excludeProtected": false,
  "tsconfig": "tsconfig.json",
  "entryPoints": ["src/index.ts"],

  "categorizeByGroup": true,
  "gitRemote": "origin",
  "includeVersion": true,
  "plugin": [
    "@shipgirl/typedoc-plugin-versions"
  ],
  "versions": {
    "stable": "1.2.0",
    "dev": "2.0.0-rc4"
  }
}
